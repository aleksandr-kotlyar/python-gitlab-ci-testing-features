import datetime

def test_check_current_week_names(self):
  week_days = ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"]
  for i in range(0, 6):
    if datetime.datetime.today().weekday() == i:
      week_days[i] = "Today"
      if i > 0:
        week_days[i - 1] = "Yesterday"
      if i < 6:
        week_days[i + 1] = "Tomorrow"
  print(week_days)
  
